{
  "name": "mark-wen-space-schema",
  "version": "1.0.0",
  "description": "gRPC schema for Mark Wen Space",
  "scripts": {
    "gen:go": "mkdir pb && protoc proto/*/*.proto --go_out=./pb",
    "gen:ts": "mkdir build && protoc --plugin='protoc-gen-ts=./node_modules/.bin/protoc-gen-ts' proto/*/*.proto --ts_out=./build",
    "nuke": "rm -rf pb build node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thousight/mark-wen-space-schema.git"
  },
  "keywords": [
    "gRPC"
  ],
  "author": "thousight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thousight/mark-wen-space-schema/issues"
  },
  "homepage": "https://github.com/thousight/mark-wen-space-schema#readme",
  "devDependencies": {
    "ts-protoc-gen": "^0.15.0"
  }
}
